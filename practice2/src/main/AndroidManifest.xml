<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.practice2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OneFragment"
            android:label="@string/title_activity_one_fragment"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TwoFragment"
            android:label="@string/title_activity_two_fragment"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ThreeFragment"
            android:label="@string/title_activity_three_fragment"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ViewPager_OneFragment"
            android:label="@string/title_activity_view_pager__one_fragment"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ViewPager_TwoFragment"
            android:label="@string/title_activity_view_pager__two_fragment"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>